SRC_DIR = $(SRC_TREE)/condor_io.new

CFLAGS = $(STD_C_FLAGS) -DNEW_PROC
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) -DNEW_PROC


SRC = stream.C sock.C reli_sock.C buffers.C
OBJ = stream.o sock.o reli_sock.o buffers.o


all_target(condor_io_lib.a test-client test-server)
library_target(condor_io_lib.a, $(OBJ), dont_release )

c_plus_target(test-client, test-client.o, condor_io_lib.a, dont_release)
c_plus_target(test-server, test-server.o, condor_io_lib.a, dont_release)

tags_target($(SRC),$(NULL))
depend_target($(SRC))
