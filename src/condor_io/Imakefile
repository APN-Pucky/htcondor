SRC_DIR = $(SRC_TREE)/condor_io.new

CFLAGS = $(STD_C_FLAGS) -DNEW_PROC
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) -DNEW_PROC

CONDOR_TYPE_SRC = sig_num.c proc.C open_flags.c fcntl_cmd.c
CONDOR_TYPE_OBJ = sig_num.o proc.o open_flags.o fcntl_cmd.o

SRC = stream.C sock.C reli_sock.C safe_sock.C buffers.C sockCache.C \
		$(CONDOR_TYPE_SRC) condor_rw.C
OBJ = stream.o sock.o reli_sock.o safe_sock.o buffers.o sockCache.o \
		$(CONDOR_TYPE_OBJ) condor_rw.o
AUTH_OBJ = auth_sock.o

all_target(libcondor.a libauth_io.a)

library_target( libcondor.a,$(OBJ))
library_target( libauth_io.a,$(AUTH_OBJ))

tags_target($(SRC),$(NULL))
depend_target($(SRC))
release:
stripped:
static:
