SRC_DIR = $(SRC_TREE)/condor_scripts
CFLAGS = $(STD_C_FLAGS)

SRC =

TARGETS = \
	condor_install condor_init condor_compile ld 

all_target($(TARGETS))
depend_target($(SRC))
tags: ALWAYS

release_target(condor_install,$(RELEASE_DIR)/sbin,EXECUTABLE_MODE)
release_target(condor_init,$(RELEASE_DIR)/sbin,EXECUTABLE_MODE)
copy_torelease_target(condor_compile,bin,condor_compile,EXECUTABLE_MODE)
copy_torelease_target(ld,lib,ld,EXECUTABLE_MODE)
link_torelease_target($(RELEASE_DIR)/lib/ld,lib,real-ld)

strip_copy_target(condor_install,$(STRIP_DIR)/sbin,condor_install,EXECUTABLE_MODE)
strip_copy_target(condor_init,$(STRIP_DIR)/sbin,condor_init,EXECUTABLE_MODE)
strip_copy_target(condor_compile,$(STRIP_DIR)/bin,condor_compile,EXECUTABLE_MODE)
strip_copy_target(ld,$(STRIP_DIR)/lib,ld,EXECUTABLE_MODE)
link_tostrip_target($(STRIP_DIR)/lib/ld,lib,real-ld)
strip_copy_target(condor_cleanckpts,../strip_ckpt_server/sbin,condor_cleanckpts,EXECUTABLE_MODE)

static_copy_target(condor_install,$(STATIC_DIR)/sbin,condor_install,EXECUTABLE_MODE)
static_copy_target(condor_init,$(STATIC_DIR)/sbin,condor_init,EXECUTABLE_MODE)
static_copy_target(condor_compile,$(STATIC_DIR)/bin,condor_compile,EXECUTABLE_MODE)
static_copy_target(ld,$(STATIC_DIR)/lib,ld,EXECUTABLE_MODE)
link_tostatic_target($(STATIC_DIR)/lib/ld,lib,real-ld)
static_copy_target(condor_cleanckpts,../static_ckpt_server/sbin,condor_cleanckpts,EXECUTABLE_MODE)

#if IS_SGI_IRIX53 || IS_SGI_IRIX62
link_torelease_target($(RELEASE_DIR)/lib/ld,lib,uld)
symlink_torelease_target(/usr/lib/c++/c++filt,lib,c++filt)
symlink_torelease_target(/usr/lib/c++/c++patch,lib,c++patch)
symlink_torelease_target(/usr/lib/c++/cfront,lib,cfront)
symlink_torelease_target(/usr/lib/c++/markc++,lib,markc++)
symlink_torelease_target(/usr/lib/c++/ptcomp,lib,ptcomp)
symlink_torelease_target(/usr/lib/c++/ptlink,lib,ptlink)

link_tostrip_target($(STRIP_DIR)/lib/ld,lib,uld)
symlink_tostrip_target(/usr/lib/c++/c++filt,lib,c++filt)
symlink_tostrip_target(/usr/lib/c++/c++patch,lib,c++patch)
symlink_tostrip_target(/usr/lib/c++/cfront,lib,cfront)
symlink_tostrip_target(/usr/lib/c++/markc++,lib,markc++)
symlink_tostrip_target(/usr/lib/c++/ptcomp,lib,ptcomp)
symlink_tostrip_target(/usr/lib/c++/ptlink,lib,ptlink)

link_tostatic_target($(STATIC_DIR)/lib/ld,lib,uld)
symlink_tostatic_target(/usr/lib/c++/c++filt,lib,c++filt)
symlink_tostatic_target(/usr/lib/c++/c++patch,lib,c++patch)
symlink_tostatic_target(/usr/lib/c++/cfront,lib,cfront)
symlink_tostatic_target(/usr/lib/c++/markc++,lib,markc++)
symlink_tostatic_target(/usr/lib/c++/ptcomp,lib,ptcomp)
symlink_tostatic_target(/usr/lib/c++/ptlink,lib,ptlink)
#endif /* IS_SGI_IRIX53 || IS_SGI_IRIX62 */

#if IS_SGI_IRIX62
link_torelease_target($(RELEASE_DIR)/lib/ld,lib,old_ld)
link_tostrip_target($(STRIP_DIR)/lib/ld,lib,old_ld)
link_tostatic_target($(STATIC_DIR)/lib/ld,lib,old_ld)
#endif

clean::
