NAME = condor_store_cred condor_get_cred condor_rm_cred condor_list_cred condor_credd
all_target( $(NAME) )

CFLAGS = $(STD_C_FLAGS) -I$(NEW_CLASSADS_INC)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) -I$(NEW_CLASSADS_INC)
LIB = $(STD_LIBS) -L$(NEW_CLASSADS_LIB) -lclassad_ns
CRED_LIB = classadUtil.o

public_c_plus_target(condor_store_cred,bin,store_cred.o $(CRED_LIB),$(LIB))

public_c_plus_target(condor_rm_cred,bin,rm_cred.o $(CRED_LIB),$(LIB))

public_c_plus_target(condor_get_cred,bin,get_cred.o $(CRED_LIB),$(LIB))

public_c_plus_target(condor_list_cred,bin,list_cred.o $(CRED_LIB),$(LIB))

OBJ = credd_main.o credd.o $(CRED_LIB) X509credentialWrapper.o

public_c_plus_target(condor_credd,sbin,$(OBJ), $(DAEMONCORE_LIB) $(LIB))

