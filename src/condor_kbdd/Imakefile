SRC_DIR = $(SRC_TREE)/condor_kbdd
CFLAGS = $(STD_C_FLAGS) -g -Wall 
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) -DNEW_PROC -fexternal-templates -Wall

SRC = kbdd.C XInterface.C
OBJ = kbdd.o XInterface.o

UTIL_LIB = ../condor_util_lib/util_lib.a 
C_PLUS_UTIL_LIB = ../condor_c++_util/cplus_lib.a ../condor_c++_util/libapi.a
CLASSAD_LIB = ../condor_classad/expr_lib.a
DAEMON_CORE_LIB = ../condor_daemon_core.V6/daemon_core.a
XDR_LIB = ../condor_xdr_lib/xdr_lib.a

STD_LIB = $(DAEMON_CORE_LIB) $(C_PLUS_UTIL_LIB) $(CLASSAD_LIB) $(UTIL_LIB) $(CEDAR_LIB) $(XDR_LIB)

#if IS_SGI_IRIX53
X_LIB = -lX11 -lXext
#else
X_LIB = XExtLibPath XLibPath
#endif
LIB = $(STD_LIB) SocketLib
LIBDIRS = ../condor_util_lib ../condor_io
LDFLAGS = $(X_LIB)

all_target(condor_kbdd)
tags_target($(SRC),$(LIBDIRS))
depend_target($(SRC))
c_plus_target(condor_kbdd,$(OBJ),$(LIB))
release_target(condor_kbdd,$(RELEASE_DIR)/bin,EXECUTABLE_MODE)
