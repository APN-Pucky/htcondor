NAME = condor_bgd
all_target( $(NAME) )

CFLAGS = $(STD_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
LIB = $(DAEMONCORE_LIB) $(STD_LIBS)

soapobjs(bgd)

OBJ = bgd_main.o bgd.o partition.o partition_mgr.o bgd_instantiate.o \
	workload.o workload_mgr.o \
	$(SOAP_OBJS)

public_c_plus_target($(NAME),sbin,$(OBJ),$(LIB))

public_copy_target(bgd_query_work_loads,libexec,bgd_query_work_loads,EXECUTABLE_MODE)
public_copy_target(bgd_generate_partition,libexec,bgd_generate_partition,EXECUTABLE_MODE)
public_copy_target(bgd_destroy_partition,libexec,bgd_destroy_partition,EXECUTABLE_MODE)
public_copy_target(bgd_available_partitions,libexec,bgd_available_partitions,EXECUTABLE_MODE)
public_copy_target(bgd_boot_partition,libexec,bgd_boot_partition,EXECUTABLE_MODE)
public_copy_target(bgd_shutdown_partition,libexec,bgd_shutdown_partition,EXECUTABLE_MODE)
public_copy_target(bgd_back_partition,libexec,bgd_back_partition,EXECUTABLE_MODE)

