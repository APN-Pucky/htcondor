SRC_DIR = $(SRC_TREE)/condor_negotiator
CFLAGS = $(STD_C_FLAGS) -DNEW_PROC
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) -DNEW_PROC

CLASSAD_LIB = ../condor_classad/expr_lib.a

SRC = negotiator.c
OBJ = negotiator.o
LIB = $(CLASSAD_LIB) ../condor_c++_util/cplus_lib.a ../condor_util_lib/util_lib.a ../condor_xdr_lib/xdr_lib.a RpcLib SocketLib KernLib
LIBDIRS = ../condor_util_lib ../condor_xdr_lib

all_target(condor_negotiator)
tags_target($(SRC),$(LIBDIRS))
depend_target($(SRC))
c_plus_target(condor_negotiator,$(OBJ),$(LIB))
release_target(condor_negotiator,$(RELEASE_DIR)/bin,EXECUTABLE_MODE)
