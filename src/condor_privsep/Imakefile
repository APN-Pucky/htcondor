all_target(privsep_client.a)

C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS)
CFLAGS = $(STD_C_FLAGS)

CLIENT_OBJ = privsep_client.UNIX.o
library_target(privsep_client.a,$(CLIENT_OBJ))

OBJ = privsep.o privsep_open.o
public_c_plus_nowrap_target(condor_privsep,sbin,$(OBJ),)

TEST_OBJ = test_privsep_open.o
c_plus_target(test_privsep_open,$(TEST_OBJ),$(STD_LIBS))

release:: all
stripped:: all
static:: all
