CFLAGS = $(STD_C_FLAGS) $(PLATFORM_C_FLAGS)
C_PLUS_FLAGS = $(STD_C_PLUS_FLAGS) -DNEW_PROC -DALLOC_DEBUG -fexternal-templates
LDFLAGS = $(LIBDBM) -lm

SRC = setflags.C status.C totals.C prettyPrint.C
OBJ = setflags.o status.o totals.o prettyPrint.o
LIB = ../condor_schedd.V6/libqmgmt.a ../condor_c++_util/cplus_lib.a ../condor_c++_util/libapi.a ../condor_io/libcondor.a ../condor_util_lib/util_lib.a ../condor_classad/expr_lib.a RpcLib SocketLib
LIBDIRS= ../condor_util_lib ../condor_xdr_lib ../condor_c++_util ../condor_classad

all_target(condor_status)
tags_target($(SRC),$(LIBDIRS))
depend_target($(SRC))
c_plus_target(condor_status,$(OBJ),$(LIB))
release_target(condor_status,$(RELEASE_DIR)/bin,SET_GID_MODE)
